{
    "manifest_version": 2,
    "name": "wand-chrome-extension",
    "version": "1.0",
    "description": "Injects a [SET DEFAULTS] button to auto-fill all time entries.",
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "browser_action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "tabs",
      "activeTab",
      "*://*.pro-unlimited.com/"
    ],
    "web_accessible_resources" : [
      "node_modules/*"
    ]
}