{
    "manifest_version": 2,
    "name": "wand-chrome-extension",
    "version": "1.0",
    "description": "Injects a [SET DEFAULTS] button to auto-fill all time entries.",
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "content_scripts": [
        {
          "matches": [
            "https://prowand.pro-unlimited.com/worker/standard/billing/billingedit/cntrl_time_create_edit_hourly-4.html*"
          ],
          "js": [
            "inject.js"
          ]
        }
    ],
    "permissions": [
      "tabs",
      "activeTab",
      "*://*.pro-unlimited.com/"
    ],
    "web_accessible_resources" : [
      "node_modules/*"
    ]
}